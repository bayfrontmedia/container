  "homepage": "https://github.com/bayfrontmedia/container",
  "license" : "MIT",
  "type": "library",
  "keywords": [
    "php",
    "container",
    "psr-11",
    "di",
    "dependency injection",
    "alias"
  ],
  "authors": [
    {
      "name": "John Robinson",
      "email": "john@bayfrontmedia.com",
      "homepage": "https://www.bayfrontmedia.com"
    }
  ],
  "support": {
    "issues": "https://github.com/bayfrontmedia/container/issues"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
